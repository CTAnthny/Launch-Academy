<%= simple_form_for @car do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.input :name %>
  <%= f.association :manufacturer %>
  <%= f.input :year %>
  <%= f.input :mileage %>
  <%= f.input :color %>
  <%= f.input :description %>
  <%= f.button :submit, "Add Car" %>
<% end %>
